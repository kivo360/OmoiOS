# OmoiOS Environment Variables
# Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:15432/app_db

# Redis
REDIS_URL=redis://localhost:16379

# LLM API (for agents)
LLM_API_KEY=sk-ant-api03-your-key-here
LLM_MODEL=anthropic/claude-sonnet-4-5-20250929

# Fireworks AI (for cost-effective doc organization, etc.)
FIREWORKS_API_KEY=your-fireworks-key-here
FIREWORKS_BASE_URL=https://api.fireworks.ai/inference/v1
FIREWORKS_MODEL=accounts/fireworks/models/gpt-oss-120b

# Auth
AUTH_JWT_SECRET_KEY=generate-a-secure-random-string-min-32-chars

# Environment
OMOIOS_ENV=local
